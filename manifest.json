{
    "manifest_version": 2,
    "name": "Cellmapper Extra Layers",
    "version": "1.2",
  
    "description": "Adds support for extra layers in cellmapper",
  
    "icons": {
      "48": "icons/icon_48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://www.cellmapper.net/map*"],
        "js": ["cellmapper.js"]
      }
    ]
}
  